  },
  "dependencies": {
    "@auth/d1-adapter": "^0.2.4",
    "@cloudflare/d1": "^1.4.1",
    "@types/node": "20.8.7",
    "@types/react": "18.2.31",
    "@types/react-dom": "18.2.14",
    "autoprefixer": "10.4.16",
    "clsx": "^2.0.0",
    "eslint": "8.52.0",
    "eslint-config-next": "13.5.6",
    "flowbite": "^2.0.0",
    "flowbite-react": "^0.6.4",
    "next": "^14.0.2",
    "next-auth": "^5.0.0-beta.3",
    "postcss": "8.4.31",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "^4.0.11",
    "recoil": "^0.7.7",
    "swr": "^2.2.4",
    "tailwind-merge": "^2.0.0",
    "tailwindcss": "3.3.3",
    "typescript": "5.2.2"
